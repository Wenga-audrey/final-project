{
  "name": "mindboost-backend",
  "version": "1.0.0",
  "description": "Backend API for Mindboost - Adaptive Learning Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "prisma generate && prisma db push && npm run db:seed && node src/index.js",
    "start": "prisma generate && prisma db push && npm run db:seed && node src/index.js",
    "start:fresh": "npm run db:reset && npm start",
    "build": "tsc",
    "start:prod": "npm run db:setup && node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node src/prisma/seed.js",
    "db:setup": "prisma generate && prisma migrate deploy",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "seed": "node scripts/seed-database.js",
    "setup": "node scripts/setup.js",
    "generate-jwt-secret": "node scripts/generate-jwt-secret.js",
    "setup-email": "node scripts/setup-email.js",
    "validate-email": "node scripts/validate-email.js",
    "test-google-ai": "node scripts/test-google-ai.js",
    "detailed-test-google-ai": "node scripts/detailed-test-google-ai.js",
    "simple-google-ai-test": "node scripts/simple-google-ai-test.js",
    "fresh-google-ai-test": "node scripts/fresh-google-ai-test.js",
    "cleanup-ai-files": "node scripts/cleanup-ai-files.js",
    "test-apis": "node scripts/test-all-apis-with-jest.js",
    "test": "node scripts/api.test.js",
    "test:all": "node scripts/run-api-tests.js"
  },
  "prisma": {
    "seed": "node src/prisma/seed.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.15.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.19",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
